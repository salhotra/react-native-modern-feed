{
  "name": "react-native-modern-feed",
  "version": "1.0.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/salhotra/react-native-modern-feed",
  "repository": {
    "type": "git",
    "url": "https://github.com/salhotra/react-native-modern-feed"
  },
  "description": "An elegant UI component for content feed.",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "expo",
    "feed",
    "list"
  ],
  "author": "Nishant Salhotra",
  "license": "MIT",
  "scripts": {
    "build": "yarn lint && tsc --outDir lib",
    "format": "prettier --write './src/**/*.tsx'",
    "lint": "tslint -c tslint.json 'src/**/**'",
    "start": "concurrently \"tsc -w --outDir example/modernFeed\" \"yarn --cwd ./example start\"",
    "check:lint": "tsc --noEmit && yarn lint",
    "release": "yarn check:lint && yarn build && npm publish"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-native": "^0.61.17",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-native": "^0.62.3",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-native": "^0.62.3"
  }
}
